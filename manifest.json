{
  "manifest_version": 3,
  "name": "Bookmafx",
  "description": "2-Columns-Filer-like Bookmark Manager",
  "version": "1.0.0",
  "author": "Kazuyuki TAKASE",
  "permissions": [
    "bookmarks"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'"
  },
  "chrome_url_overrides": {
    "bookmarks": "index.html"
  }
}
